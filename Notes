
# Minimiser le controle pour seulement un seul degré de liberté en rajoutant index [],
# En sachant que :
# ddl pelvis = 1 [1] # ddl thorax = 1 [2], 2 [3], 3 [4] # ddl l'humerus = 1 [5], 2 [6], 3 [7] # ddl ulna = 1 [8] # ddl radius = 1 [9] # ddl hand = 1 [10], 2 [11]

  objective_functions.add(ObjectiveFcn.Lagrange.MINIMIZE_CONTROL, key="tau", weight=1000, phase=0, index=[3])